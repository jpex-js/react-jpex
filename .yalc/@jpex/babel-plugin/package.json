{
  "name": "@jpex/babel-plugin",
  "version": "1.0.0-65e5c9fb",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn build && BABEL_DISABLE_CACHE=1 ava",
    "test:debug": "ava debug",
    "coverage": "nyc ava",
    "lint": "eslint './src/**/*.ts' && tsc --noEmit",
    "build:prepare": "rm -rf dist",
    "build:js": "rollup --config ./rollup.config.js",
    "build": "yarn build:prepare && yarn build:js",
    "prepublishOnly": "yarn build"
  },
  "repository": "git@github.com:jpex-js/babel.git",
  "publishConfig": {
    "access": "public"
  },
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts}": [
        "eslint -c ./.eslintrc.js",
        "git add"
      ]
    }
  }
}
